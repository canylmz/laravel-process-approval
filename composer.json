{
    "name": "canylmz/laravel-process-approval",
    "description": "A package to handle process approval flow in a laravel application with Laravel 12.x support.",
    "type": "laravel-package",
    "homepage": "https://github.com/canylmz/laravel-process-approval",
    "license": "MIT",
    "keywords": [
        "approval",
        "approve",
        "flow",
        "procedure",
        "laravel",
        "laravel12"
    ],
    "autoload": {
        "psr-4": {
            "Canylmz\\LaravelProcessApproval\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Can YÄ±lmaz",
            "email": "canylmz6134@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/auth": "^10.0|^11.0|^12.0",
        "illuminate/broadcasting": "^10.7|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.24|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Canylmz\\LaravelProcessApproval\\LaravelProcessApprovalServiceProvider"
            ],
            "aliases": {
                "ProcessApproval": "Canylmz\\LaravelProcessApproval\\Facade"
            },
            "dont-discover": [
            ]
        }
    },
    "branches": {
        "stable": {
            "name": "master"
        }
    },
    "version": "1.0.0",
    "minimum-stability": "dev",
    "prefer-stable": true
}
